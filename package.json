{
  "name": "vue-nest-ssr",
  "version": "0.0.1",
  "description": "Template for vue + nest app with server side rendering",
  "main": "packages/server/src/main.ts",
  "scripts": {
    "dev": "vite --config ./packages/server/vite.config.js",
    "build:server": "vite build --config ./packages/server/vite.config.js",
    "build:client": "vite build --config ./packages/client/vite.config.js",
    "build": "yarn build:client && yarn build:server",
    "preview": "node dist/server/main.mjs",
    "format": "prettier --write ./packages/**/*.{ts,vue,json,js,css,scss}",
    "lint:client": "eslint -c packages/client/.eslintrc.js \"packages/client/**/*.{ts,vue,scss}\" --fix",
    "lint:server": "eslint -c packages/server/.eslintrc.js \"packages/server/**/*.ts\" --fix",
    "lint": "yarn lint:client && yarn lint:server"
  },
  "keywords": [],
  "author": "dan.exxx1st@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@apollo/server": "^4.7.4",
    "@as-integrations/fastify": "^2.0.0",
    "@css-render/vue3-ssr": "^0.15.12",
    "@fastify/compress": "^6.4.0",
    "@fastify/static": "^6.10.2",
    "@nestjs/apollo": "^12.0.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.3",
    "@nestjs/platform-fastify": "^10.0.3",
    "@prisma/client": "^4.16.1",
    "@vicons/fluent": "^0.12.0",
    "@vue/apollo-option": "^4.0.0-beta.5",
    "@vue/apollo-ssr": "4.0.0-beta.5",
    "fastify": "^4.18.0",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sirv": "^2.0.3",
    "ts-morph": "^19.0.0",
    "vfonts": "^0.0.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "~0.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/compression": "^1.7.2",
    "@types/jest": "29.5.1",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.15.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^4.16.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-node": "^3.0.2"
  },
  "prisma": {
    "schema": "./packages/server/prisma/schema.prisma"
  }
}
